{
    "name": "amadeco/magento2-patches",
    "type": "patchset",
    "version": "1.1.3",
    "require": {
        "creativestyle/composer-plugin-patchset": "^2.0",
        "creativestyle/magento-patches": "^1.3"
    },
    "extra": {
        "patchset": {
            "aheadworks/module-automatic-related-products": [
                {
                    "description": "FIX: Zend_Json usage deprecated since 2.4.6 - Replace by Laminas Json.",
                    "filename": "aheadworks/module-automatic-related-products/replace-zend-laminas-json.patch",
                    "version": "*"
                }
            ],
            "comwrap/module-elasticsuite-blog-search": [
                {
                    "description": "FIX: Magento 2.4.x support #18",
                    "filename": "comwrap/module-elasticsuite-blog-search/collection.patch",
                    "version": "*"
                }
            ],
            "magento/framework": [
                {
                    "description": "IMPROVEMENTS: Import styles for enabled modules only | https://github.com/vasilii-b/magento2-frontend-improvements",
                    "filename": "magento/framework/import-styles-for-enabled-modules-only.patch",
                    "version": "*"
                }
            ],
            "magento/magento2-base": [
                {
                    "description": "IMPROVEMENTS: Add patch jQuery scroll passive for mobile scrolling performance #35613 - 1",
                    "filename": "magento/magento2-base/jquery-scroll-passive-1.patch",
                    "version": "*"
                }
            ],
            "magento/module-checkout": [
                {
                    "description": "Fix displaying error message about no active cart after placing order",
                    "filename": "magento/module-checkout/m245-fix-customer-has-no-active-cart.patch",
                    "version-constraint": ">=100.4.5"
                }
            ],
            "magento/module-inventory-sales": [
                {
                    "description": "FIX: We don't have as many quantity as you requested, but we'll back order the remaining 1 #23047",
                    "filename": "magento/module-inventory-sales/BackOrderNotifyCustomerCondition.patch",
                    "version": "*"
                }
            ],
            "module-inventory-source-deduction-api": [
                {
                    "description": "FIX: Multisource Inventory, can't ship items when using default Stocks #21646",
                    "filename": "magento/module-inventory-source-deduction-api/SourceDeductionService.patch",
                    "version": "*"
                }
            ],
            "magento/module-theme": [
                {
                    "description": "IMPROVEMENTS: Add patch jQuery scroll passive for mobile scrolling performance #35613 - 2",
                    "filename": "magento/module-theme/jquery-scroll-passive-2.patch",
                    "version": "*"
                }
            ],
            "mageworx/module-seomarkup" : [
                {
                    "description": "FIX: Support multiselect attribute for color information in SEO markup",
                    "filename": "mageworx/module-seomarkup/mageworx-seomarkup-support-color-array.patch",
                    "version": "*"
                },
                {
                    "description": "Fix product reviews on category page (LEDS)",
                    "filename": "mageworx/module-seomarkup/mageworx-seomarkup-fix-product-review.patch",
                    "version-constraint": ">=2.6.1"
                }
            ],
            "mirasvit/module-core": [
                {
                    "description": "Remove license verification",
                    "filename": "mirasvit/module-core/remove-license-verification.patch",
                    "version": "*"
                }
            ],
            "mirasvit/module-email-designer": [
                {
                    "description": "FIX: VariableProvider.php: PHP 8 compatibility (explode(): Passing null to parameter #2 ($string))",
                    "filename": "mirasvit/module-email-designer/VariableProvider.patch",
                    "version": "*"
                }
            ],
            "owebia/magento2-module-shared-php-config": [
                {
                    "description": "FIX: TypeError: Owebia\\SharedPhpConfig\\Model\\Parser::parse(): Argument #2 ($configuration) must be of type string, null given",
                    "filename": "owebia/magento2-module-shared-php-config/nullable-type-parser.patch",
                    "version": "*"
                }
            ],
            "owebia/magento2-module-shared-license": [
                {
                    "description": "FIX: Removing the checking of the license",
                    "filename": "owebia/magento2-module-shared-license/removing-checking-license.patch",
                    "version": "*"
                }
            ]
        }
    }
}